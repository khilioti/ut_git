	<calculatedField>
		<dataPath>ПроцентВыдачи</dataPath>
		<expression>ВЫБОР
			КОГДА (ВсегоЧеков - ЧекиСБонуснойКартой) &lt;= 0
									ТОГДА 0
			ИНАЧЕ (Пластик + Виртуальная) / (ВсегоЧеков - ЧекиСБонуснойКартой) * 100
			КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Процент выдачи,%</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<calculatedField>
		<dataPath>ПроникновениеБК</dataPath>
		<expression>ВЫБОР
		КОГДА ВсегоЧеков = 0
						ТОГДА 0
					ИНАЧЕ (ЧекиСБонуснойКартой / ВсегоЧеков)*100
							КОНЕЦ 
</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Проникновение БК,%</v8:content>
			</v8:item>
		</title>
	</calculatedField>
